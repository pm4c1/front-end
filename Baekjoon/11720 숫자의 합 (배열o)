public static void main(String[] args) {
		
        Scanner sc = new Scanner(System.in);

        int num = sc.nextInt(); // 숫자 입력받음;
        int sum = 0;
        String[] n = sc.next().split(""); // 문자열로 받은걸 잘라서 배열에 담음

        for (int i = 0; i < num; i++) {
            sum += Integer.parseInt(n[i]);  // n -> 10진 ( 기본이 10진 )
        }
	
        System.out.println(sum);
        
    }
