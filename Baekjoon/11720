public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt(); // 숫자의 개수를 입력받음 ex) 5
		String a = sc.next();	// 문자열을 입력 받음	 ex) 12345
		
		int sum = 0;
		for(int i=0; i<num; i++) {
			sum += a.charAt(i)-'0';
		// charAt(0)부터 charAt(num)까지 누적합을 구함
			
		// charAt()은 해당 문자의 아스키코드 값을 반환하기에
		// -'0'을 해주어 입력받은 숫자 값을 그대로 사용가능
			
		}
		System.out.println(sum);
	}
